{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3001",
    "build": "vue-tsc --noEmit && vite build &&  cp -r node_modules/shiki dist/",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "@guzh/css-preset": "workspace:*",
    "@guzh/pinia-store": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.14",
    "shiki": "^0.11.1",
    "typescript": "~4.6.3",
    "vite": "3.2.2",
    "vue-tsc": "^0.34.7",
    "@vitejs/plugin-vue-jsx": "1.3.10",
    "unplugin-vue-define-options": "0.12.6"
  }
}
