{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["@guzh/css-preset#stub","^build"],
      "outputs": ["dist/**", ".nuxt/**", ".output/**"]
    },
    "test": {
      "dependsOn": ["@guzh/css-preset#stub"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "stub": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["@guzh/css-preset#stub"],
      "cache": false
    },
    "start": {
      "dependsOn": ["build"]
    }
  }
}
